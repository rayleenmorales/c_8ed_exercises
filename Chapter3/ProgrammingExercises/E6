#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
	double mass, density, volume;
	
	cout<<"Enter the mass(g): ";
	cin>>mass;
	cout<<endl;
	
	cout<<"Enter the density(g/cm^3): ";
	cin>>density;
	cout<<endl;
	
	volume = mass/density;
	
	cout<<fixed<<showpoint<<setprecision(2);
	
	cout<<"Volume of the object is "<<volume<<" cm^3."<<endl;
	
	return 0;
}
